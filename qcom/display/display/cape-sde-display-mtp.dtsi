};

&dsi_r66451_amoled_cmd {
	qcom,panel-supply-entries = <&dsi_panel_pwr_supply_sim>;
	qcom,panel-sec-supply-entries = <&dsi_panel_pwr_supply_sim>;
	qcom,mdss-dsi-bl-pmic-control-type = "bl_ctrl_dcs";
	qcom,mdss-dsi-sec-bl-pmic-control-type = "bl_ctrl_dcs";
	qcom,mdss-dsi-bl-min-level = <1>;
	qcom,mdss-dsi-bl-max-level = <4095>;
	qcom,mdss-brightness-max-level = <8191>;
	qcom,mdss-dsi-bl-inverted-dbv;
	qcom,platform-reset-gpio = <&tlmm 0 0>;
	qcom,platform-sec-reset-gpio = <&tlmm 4 0>;
};

&qupv3_se4_spi {
	novatek@0 {
		panel = <&dsi_nt36523n_lcd_144hz_hyper_video_cphy
			&dsi_nt36523n_lcd_144hz_video_cphy>;
	};
};
&qupv3_se2_i2c {
	status = "ok";

	#address-cells = <1>;
	#size-cells = <0>;
        ktz8866_bkl@11 {
                compatible = "kinetic,ktz8866_bkl";
                reg = <0x11>;
                pinctrl-names = "bkl_active","bkl_suspend";
                pinctrl-0 = <&bkl_init_active>;
                pinctrl-1 = <&bkl_init_suspend>;
                bkl,hwen-gpio = <&tlmm 62 0>;
                bkl,enp-gpio = <&tlmm 61 0>;
                bkl,enn-gpio = <&tlmm 65 0>;
	};
};

&spmi_bus {
	qcom,pm8350b@3 {
		qcom,amoled-ecm@f900 {
			display-panels = <&dsi_r66451_amoled_cmd_cphy
					&dsi_r66451_amoled_144hz_cmd_cphy
					&dsi_r66451_amoled_144hz_video_cphy
					&dsi_r66451_amoled_video_cphy
					&dsi_r66451_amoled_120hz_cmd_cphy
					&dsi_nt36523n_lcd_144hz_video_cphy
					&dsi_nt36523n_lcd_144hz_hyper_video_cphy
					&dsi_r66451_amoled_120hz_video_cphy>;
				//	status= "disabled";

		};
	};
};
