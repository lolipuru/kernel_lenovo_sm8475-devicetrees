#include "cape-sde-display.dtsi"

&dsi_nt36523n_lcd_144hz_video_cphy {
        qcom,panel-supply-entries = <&dsi_panel_pwr_supply_extvdd>;
        qcom,mdss-dsi-bl-pmic-control-type = "bl_ctrl_external";
        qcom,mdss-dsi-bl-min-level = <13>;
        qcom,mdss-dsi-bl-max-level = <2047>;
        qcom,mdss-brightness-max-level = <255>;
        qcom,platform-reset-gpio = <&tlmm 0 0>;
	qcom,platform-te-gpio = <&tlmm 86 0>;
        qcom,bias-enp-gpio = <&tlmm 61 0>;
        qcom,bias-enn-gpio = <&tlmm 65 0>;
};

